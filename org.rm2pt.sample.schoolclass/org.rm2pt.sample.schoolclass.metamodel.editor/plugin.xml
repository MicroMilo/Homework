<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated schoolclass -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.rm2pt.sample.schoolclass.metamodel.schoolclass.presentation.SchoolclassModelWizardID"
            name="%_UI_SchoolclassModelWizard_label"
            class="org.rm2pt.sample.schoolclass.metamodel.schoolclass.presentation.SchoolclassModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/SchoolclassModelFile.gif">
         <description>%_UI_SchoolclassModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated schoolclass -->
      <editor
            id="org.rm2pt.sample.schoolclass.metamodel.schoolclass.presentation.SchoolclassEditorID"
            name="%_UI_SchoolclassEditor_label"
            icon="icons/full/obj16/SchoolclassModelFile.gif"
            extensions="schoolclass"
            class="org.rm2pt.sample.schoolclass.metamodel.schoolclass.presentation.SchoolclassEditor"
            contributorClass="org.rm2pt.sample.schoolclass.metamodel.schoolclass.presentation.SchoolclassActionBarContributor">
      </editor>
   </extension>

</plugin>
